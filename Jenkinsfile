                echo 'linting ended'
            }
        }
        stage("build") {
            steps {
                 echo 'build started'
                 echo 'build ended'
            }
        }
        stage("test") {
            steps {
                 echo 'test started'
                 echo 'test ended'
            }
        }
         stage("deployment") {
            steps {
                 echo 'deployment started'
                 echo 'deployment ended'
            }
        }
    }
}